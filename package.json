{
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "dotenv": "^16.4.7",
    "prisma": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4"
  },
  "scripts": {
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "prisma:generate": "prisma generate --config ./prisma/prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --name init --config ./prisma/prisma.config.ts",
    "prisma:status": "prisma migrate status --config ./prisma/prisma.config.ts",
    "frontend:dev": "yarn --cwd frontend dev",
    "frontend:build": "yarn --cwd frontend build",
    "frontend:lint": "yarn --cwd frontend lint",
    "frontend:codegen": "yarn --cwd frontend codegen"
  }
}
